<!-- 系统切换 -->
<script>
    // 代表全局当前显示的是什么操作系统, 默认为win
    let globalSys = {
        value: 'win'
    }; 

    //监听 globalSys的变化
    const globalSysObserver =  new Proxy(globalSys, {
        get(target, key, receiver) {
            return target[key];
        },
        set(target, key, value) {
            changeSysView(value);
            target[key] = value;
        },
    });

    // 改变显示试图
    function changeSysView(go) {
        const sysTabBoxS = document.querySelectorAll('.sys-tab-box');
        switch (go) {
            case 'win':
                $('.sys-win').fadeIn(150)
                $('.sys-mac').hide();
                break;
            case 'mac':
                $('.sys-mac').fadeIn(150)
                $('.sys-win').hide();
                break;
            default:
                throw new Error('当前操作系统不存在');
                break;
        }
        $('.changeSystemBox').attr('data-sys', go)
        $('.sys-tab-box').attr('data-sys', go)
    }

    $('.changeSystemBox .bar').on('click' ,function() {
        const currentSys = this.parentElement.getAttribute('data-sys')
            let goSys;
            if(currentSys === 'win') {
                goSys = 'mac'
            }else if(currentSys === 'mac') {
                goSys = 'win'
            }
            globalSysObserver.value = goSys; //修改全局当前操作系统.
    })

    $('.sys-tab').on('click', function() {
        const  goSys = this.getAttribute('data-go');
        globalSysObserver.value = goSys
    })

    if (/iPhone|Mac/i.test(navigator.userAgent)) {
        globalSysObserver.value = 'mac'
    } else {
        globalSysObserver.value = 'win'
    }


</script>

<!-- 支付:目前仅仅支持strip -->
<script>
    // 支付的状态
    const payStatus = {
        value: 'success', // 三个值,  padding, success, error
    }
    //监听 支付状态改变
    const payStatusObserver =  new Proxy(payStatus, {
        get(target, key, receiver) {
            return target[key];
        },
        set(target, key, value) {
            changeBuyAwaitModelView(value)
            target[key] = value;
        },
    });
    
    // 根据支付状态，弹窗显示不同的内容
    function changeBuyAwaitModelView(status){
        switch (status) {
            case 'padding':
                $('.buyAwaitModel .padding').show().siblings().hide()
                break;
            case 'success':
                $('.buyAwaitModel .success').show().siblings().hide()
                break;
            case 'error':
                $('.buyAwaitModel .error').show().siblings().hide()
                break;
            default:
                throw new Error('支付状态只有, padding | success | error 三种状态')
                break;
        }
    }
    
    // 支付等待时候的弹窗
    let buyAwaitModel = document.querySelector('.buyAwaitModel');
    // 给弹窗绑定点击时间，当支付失败的时候可以手动点击关闭
    buyAwaitModel.addEventListener('click', function(e) {
        if(payStatusObserver.value === 'error') {
            buyAwaitModel.style.display = 'none'
        }
    })
    
    
    // 控制使用哪个支付
    function handlePay(trading_platform, productInfo) {
        switch (trading_platform) {
            case 'paypal':
                paypalCheckout(productInfo)
                break;
            case 'stripe':
                stripeCheckout(productInfo)
                break;
            case 'shareit':
                shareitCheckout(productInfo)
                break;
            default:
                throw new Error('没有这个支付方式')
                break;
        }
    }
    
    /*
    stripe购买
    
    productInfo 包含以下值
      sku_id: 产品的skuId，
      isSubscribe: 是否订阅产品
      effective_months: 产品有效期 以月为单位
      title: 产品名
      system: 操作系统
      description: 产品描述
      price: 价格，
    */
    async function stripeCheckout(productInfo) {
        try {
            let orderData = await storeOrder(productInfo.sku_id, productInfo.isSubscribe);
            let order_id = orderData.id;
            // Which Payment Method
            if (productInfo.effective_months > 0) {
                let route = `/api/payment/${order_id}/stripe/plan`;
                // Test - Stripe Daily Plan
                if (productInfo.sku_id === 24) {
                    route = `/api/payment/${order_id}/stripe/daily_plan`;
                }
                // 获取计划信息
                const planData = await $.ajax({
                    url: route,
                    method: "POST",
                    headers: {
                        'Content-Type': 'application/json'
                    },
                })
    
    
                const sessionData = await $.ajax({
                    url: "/api/payment/stripe/checkout-session",
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    data: JSON.stringify({
                        planId: planData.plan_id,
                        no: planData.no,
                        effective_months: productInfo.effective_months
                    })
                })
    
                let stripe = Stripe('pk_live_51LQpEiABsRLw6VKvb1c3RrHZ78K8D36Ime5Lzcom89aWw6ILLqfMLmvnMt1a4VbqV3tjkrl2Lwb7oLpw3E5DiBH600QRB8VpwD');
                await stripe.redirectToCheckout({
                    sessionId: sessionData.sessionId
                });
            } else {
                // 这个为终身计划
                const sessionData = await $.ajax({
                    url: "/api/payment/stripe/checkout-session",
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    data: JSON.stringify({
                        effective_months: productInfo.effective_months,
                        order_id: orderData.id
                    })
                })
                
                let stripe = Stripe('pk_live_51LQpEiABsRLw6VKvb1c3RrHZ78K8D36Ime5Lzcom89aWw6ILLqfMLmvnMt1a4VbqV3tjkrl2Lwb7oLpw3E5DiBH600QRB8VpwD');
                await stripe.redirectToCheckout({
                    sessionId: sessionData.sessionId
                });
            }
        } catch (error) {
            console.log(error);
            payStatusObserver.value = 'error';
        }
    }
    
    // Store An Order
    async function storeOrder(sku_id, isSubscribe, couponCode=null) {
            const response = await $.ajax({
                url: '/api/orders',
                method:"POST",
                headers: {
                    'Content-Type': 'application/json'
                },
                data: JSON.stringify({
                    items: [{
                        sku_id,
                        amount: 1,
                    }],
                    coupon_code: couponCode,
                    subscribe: isSubscribe,
                }),
            })
            return response;
        }
    
    // 防抖
    function debounce(fn, delay = 500) {
        // timer 是在闭包中的
        let timer = null;
        
        return function() {
            if (timer) {
                clearTimeout(timer)
            }
            timer = setTimeout(() => {
                fn.apply(this, arguments)
                timer = null
            }, delay)
        }
    }
    
    function googleAnalyticsCode(sku_id, title, price, system, description) {
            // GUA
            gtag('event', 'begin_checkout', {
                "items": [{
                    "id": sku_id,
                    "name": title,
                    "quantity": 1,
                    "price": price,
                    "category": system,
                    "variant": description,
                }],
            });
    
            // GA4
            gtag('event', 'begin_checkout', {
                "items": [{
                    "item_id": sku_id,
                    "item_name": title,
                    "quantity": 1,
                    "price": price,
                    "item_category": system,
                    "item_variant": description,
                }],
            });
        }
    
    // 给购买按钮绑定点击事件
    $('.buy-btn').on('click', debounce(function() {
        let productInfo = {
            sku_id: $(this).attr('data-sku_id'), //产品的skuId，
            isSubscribe: true,  //是否订阅产品 ，这里默认订阅
            effective_months: $(this).attr('data-effective_months'), //产品有效期 以月为单位
            title: $(this).attr('data-title'), //产品名
            system: $(this).attr('data-system'), //操作系统
            description: $(this).attr('data-description'), //产品描述
            price: $(this).attr('data-price')
        }
        let trading_platform = $(this).attr('data-platform');  //支付平台
        buyAwaitModel.style.display = 'block';
        payStatusObserver.value = 'padding'
        handlePay(trading_platform, productInfo); //执行支付
        googleAnalyticsCode(productInfo.sku_id, productInfo.title, productInfo.price, productInfo.system, productInfo.description) ;
    },200))
        
    </script>

<script>
let isOne = false; // 判断是否为第一个youtube-nav
let timer; //定时器
$('#section2 .youtube-nav').on('click', function() {
    $(this).addClass('active').siblings().removeClass('active');
    $('body,html').animate({
        scrollTop: $('#' + $(this).attr('data-go')).offset().top - 60
    }, 300);
    clearTimeout(timer)
    timer = setTimeout(() => {
        isOne = true
    }, 300);
})
$('#sticky-nav .youtube-nav').on('click', function() {
    $('body,html').animate({
        scrollTop: $('#' + $(this).attr('data-go')).offset().top - 60
    }, 300);
})

window.addEventListener('scroll', function() {
    if(window.scrollY <= $('#Download-Spotify-Songs').offset().top) {
        if(isOne) {
            $('#section2 .youtube-nav').eq(0).addClass('active').siblings().removeClass('active')
            isOne = false
        }
    }
});


